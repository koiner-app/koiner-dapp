{
  "type": "object",
  "elements": [
    {
      "type": "TableView",
      "options": {
        "search": {
          "provider": "tokenOperations"
        }
      },
      "elements": [
        {
          "type": "Attribute",
          "label": "Age",
          "scope": "#/properties/timestamp",
          "options": {
            "format": "age",
            "mapping": "timestamp",
            "class": "attribute-wrapper--age"
          }
        },
        {
          "type": "Attribute",
          "label": "From",
          "scope": "#/properties/from",
          "options": {
            "format": "address",
            "mapping": "from",
            "class": "attribute-wrapper--address"
          }
        },
        {
          "type": "Attribute",
          "scope": "#/properties/name",
          "options": {
            "format": "tokenEventName",
            "class": "attribute--icon"
          }
        },
        {
          "type": "Attribute",
          "label": "To",
          "scope": "#/properties/to",
          "options": {
            "format": "address",
            "mapping": "to",
            "class": "attribute-wrapper--address"
          }
        },
        {
          "type": "Attribute",
          "label": "Value",
          "scope": "#/properties/value",
          "options": {
            "format": "tokenAmount",
            "mappings": {
              "contract.id": "contract.id",
              "contract.name": "contract.name",
              "contract.symbol": "contract.symbol",
              "contract.decimals": "contract.decimals"
            }
          }
        },
        {
          "type": "Attribute",
          "label": "Token",
          "scope": "#/properties/contractId",
          "mapping": "contract.id",
          "class": "attribute-wrapper--address",
          "options": {
            "format": "token",
            "mappings": {
              "contract.id": "contract.id",
              "contract.name": "contract.name",
              "contract.symbol": "contract.symbol",
              "contract.decimals": "contract.decimals"
            },
            "screenSize": "gt-md"
          }
        },
        {
          "type": "AttributeGroup",
          "label": "",
          "scope": "#/properties/links",
          "elements": [
            {
              "type": "Attribute",
              "scope": "#/properties/blockHeight",
              "options": {
                "route": "block",
                "format": "link",
                "icon": "token",
                "linkParams": [
                  "height"
                ],
                "mappings": {
                  "height": "blockHeight"
                }
              }
            },
            {
              "type": "Attribute",
              "scope": "#/properties/transactionId",
              "options": {
                "route": "transaction",
                "format": "link",
                "icon": "receipt",
                "linkParams": [
                  "id"
                ],
                "mappings": {
                  "id": "transactionId"
                }
              }
            }
          ]
        }
      ]
    }
  ]
}
