        {
          "type": "Attribute",
          "label": "Network",
          "scope": "#/properties/receipt/properties/networkBandwidthUsed",
          "options": {
            "mapping": "receipt.networkBandwidthUsed"
          }
        },
        {
          "type": "Attribute",
          "label": "Mana used",
          "scope": "#/properties/receipt/properties/rcUsed",
          "options": {
            "format": "manaTokenAmount",
            "mapping": "receipt.rcUsed"
          }
        }
      ]
    }
  ]
}
