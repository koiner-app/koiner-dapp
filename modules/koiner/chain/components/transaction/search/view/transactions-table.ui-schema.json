          "scope": "#/properties/timestamp",
          "options": {
            "format": "age",
            "mapping": "timestamp"
          }
        },
        {
          "type": "Attribute",
          "label": "ID",
          "scope": "#/properties/id",
          "options": {
            "route": "transaction",
            "format": "link",
            "linkParams": ["id"],
            "mappings": {
              "id": "id"
            }
          }
        },
        {
          "type": "Attribute",
          "label": "Block",
          "scope": "#/properties/blockHeight",
          "options": {
            "route": "block",
            "format": "link",
            "linkParams": ["height"],
            "mappings": {
              "height": "blockHeight"
            }
          }
        },
        {
          "type": "Attribute",
          "label": "Ops",
          "scope": "#/properties/operationCount"
        },
        {
          "type": "Attribute",
