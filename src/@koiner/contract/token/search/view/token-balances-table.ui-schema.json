          "provider": "tokenBalances"
        }
      },
      "elements": [
        {
          "type": "Attribute",
          "label": "Address",
          "scope": "#/properties/addressId",
          "options": {
            "format": "address"
          }
        },
        {
          "type": "Attribute",
          "scope": "#/properties/balance",
          "options": {
            "format": "tokenAmount",
            "mappings": {
              "contract.id": "contract.id",
              "contract.name": "contract.name",
              "contract.symbol": "contract.symbol",
              "contract.decimals": "contract.decimals"
            }
          }
        }
      ]
    }
  ]
}
