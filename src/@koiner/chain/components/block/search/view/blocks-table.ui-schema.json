{
  "type": "object",
  "elements": [
    {
      "type": "TableView",
      "options": {
        "search": {
          "provider": "blocks"
        }
      },
      "elements": [
        {
          "type": "Attribute",
          "label": "Height",
          "scope": "#/properties/height",
          "options": {
            "route": "block",
            "format": "link",
            "linkParams": ["height"],
            "mapping": "header.height",
            "mappings": {
              "height": "header.height"
            }
          }
        },
        {
          "type": "Attribute",
          "label": "Age",
          "scope": "#/properties/timestamp",
          "options": {
            "format": "age",
            "mapping": "header.timestamp"
          }
        },
        {
          "type": "Attribute",
          "label": "Transactions",
          "scope": "#/properties/transactionCount"
        },
        {
          "type": "Attribute",
          "label": "Events",
          "scope": "#/properties/eventCount",
          "options": {
            "mapping": "receipt.eventCount"
          }
        },
        {
          "type": "Attribute",
          "label": "Compute used",
          "scope": "#/properties/computeBandwidthUsed",
          "options": {
            "mapping": "receipt.computeBandwidthUsed"
          }
        },
        {
          "type": "Attribute",
          "label": "Storage used",
          "scope": "#/properties/diskStorageUsed",
          "options": {
            "mapping": "receipt.diskStorageUsed"
          }
        },
        {
          "type": "Attribute",
          "label": "Network used",
          "scope": "#/properties/networkBandwidthUsed",
          "options": {
            "mapping": "receipt.networkBandwidthUsed"
          }
        }
      ]
    }
  ]
}
