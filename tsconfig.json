{
  "extends": "@quasar/app-webpack/tsconfig-preset",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@koiner": ["./src/@koiner"],
      "@koiner/*": ["./src/@koiner/*"],
      "@appvise": ["./src/@appvise"],
      "@appvise/*": ["./src/@appvise/*"],
      "src/*": ["src/*"],
      "app/*": ["*"],
      "components/*": ["src/components/*"],
      "layouts/*": ["src/layouts/*"],
      "pages/*": ["src/pages/*"],
      "assets/*": ["src/assets/*"],
      "boot/*": ["src/boot/*"],
      "stores/*": ["src/stores/*"]
    }
  }
}
